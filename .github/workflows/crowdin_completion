#!/usr/bin/env bash
#
# crowdin Bash Completion
# =======================
#
# Bash completion support for the `crowdin` command,
# generated by [picocli](http://picocli.info/) version 4.6.1.
#
# Installation
# ------------
#
# 1. Source all completion scripts in your .bash_profile
#
#   cd $YOUR_APP_HOME/bin
#   for f in $(find . -name "*_completion"); do line=". $(pwd)/$f"; grep "$line" ~/.bash_profile || echo "$line" >> ~/.bash_profile; done
#
# 2. Open a new bash console, and type `crowdin [TAB][TAB]`
#
# 1a. Alternatively, if you have [bash-completion](https://github.com/scop/bash-completion) installed:
#     Place this file in a `bash-completion.d` folder:
#
#   * /etc/bash-completion.d
#   * /usr/local/etc/bash-completion.d
#   * ~/bash-completion.d
#
# Documentation
# -------------
# The script is called by bash whenever [TAB] or [TAB][TAB] is pressed after
# 'crowdin (..)'. By reading entered command line parameters,
# it determines possible bash completions and writes them to the COMPREPLY variable.
# Bash then completes the user input if only one entry is listed in the variable or
# shows the options if more than one is listed in COMPREPLY.
#
# References
# ----------
# [1] http://stackoverflow.com/a/12495480/1440785
# [2] http://tiswww.case.edu/php/chet/bash/FAQ
# [3] https://www.gnu.org/software/bash/manual/html_node/The-Shopt-Builtin.html
# [4] http://zsh.sourceforge.net/Doc/Release/Options.html#index-COMPLETE_005fALIASES
# [5] https://stackoverflow.com/questions/17042057/bash-check-element-in-array-for-elements-in-another-array/17042655#17042655
# [6] https://www.gnu.org/software/bash/manual/html_node/Programmable-Completion.html#Programmable-Completion
# [7] https://stackoverflow.com/questions/3249432/can-a-bash-tab-completion-script-be-used-in-zsh/27853970#27853970
#

if [ -n "$BASH_VERSION" ]; then
  # Enable programmable completion facilities when using bash (see [3])
  shopt -s progcomp
elif [ -n "$ZSH_VERSION" ]; then
  # Make alias a distinct command for completion purposes when using zsh (see [4])
  setopt COMPLETE_ALIASES
  alias compopt=complete

  # Enable bash completion in zsh (see [7])
  autoload -U +X compinit && compinit
  autoload -U +X bashcompinit && bashcompinit
fi

# CompWordsContainsArray takes an array and then checks
# if all elements of this array are in the global COMP_WORDS array.
#
# Returns zero (no error) if all elements of the array are in the COMP_WORDS array,
# otherwise returns 1 (error).
function CompWordsContainsArray() {
  declare -a localArray
  localArray=("$@")
  local findme
  for findme in "${localArray[@]}"; do
    if ElementNotInCompWords "$findme"; then return 1; fi
  done
  return 0
}
function ElementNotInCompWords() {
  local findme="$1"
  local element
  for element in "${COMP_WORDS[@]}"; do
    if [[ "$findme" = "$element" ]]; then return 1; fi
  done
  return 0
}

# The `currentPositionalIndex` function calculates the index of the current positional parameter.
#
# currentPositionalIndex takes three parameters:
# the command name,
# a space-separated string with the names of options that take a parameter, and
# a space-separated string with the names of boolean options (that don't take any params).
# When done, this function echos the current positional index to std_out.
#
# Example usage:
# local currIndex=$(currentPositionalIndex "mysubcommand" "$ARG_OPTS" "$FLAG_OPTS")
function currentPositionalIndex() {
  local commandName="$1"
  local optionsWithArgs="$2"
  local booleanOptions="$3"
  local previousWord
  local result=0

  for i in $(seq $((COMP_CWORD - 1)) -1 0); do
    previousWord=${COMP_WORDS[i]}
    if [ "${previousWord}" = "$commandName" ]; then
      break
    fi
    if [[ "${optionsWithArgs}" =~ ${previousWord} ]]; then
      ((result-=2)) # Arg option and its value not counted as positional param
    elif [[ "${booleanOptions}" =~ ${previousWord} ]]; then
      ((result-=1)) # Flag option itself not counted as positional param
    fi
    ((result++))
  done
  echo "$result"
}

# Bash completion entry point function.
# _complete_crowdin finds which commands and subcommands have been specified
# on the command line and delegates to the appropriate function
# to generate possible options and subcommands for the last specified subcommand.
function _complete_crowdin() {
  local cmds0=(upload)
  local cmds1=(upload)
  local cmds2=(download)
  local cmds3=(download)
  local cmds4=(list)
  local cmds5=(lint)
  local cmds6=(generate)
  local cmds7=(generate)
  local cmds8=(status)
  local cmds9=(string)
  local cmds10=(glossary)
  local cmds11=(tm)
  local cmds12=(pre-translate)
  local cmds13=(upload translations)
  local cmds14=(upload sources)
  local cmds15=(upload translations)
  local cmds16=(upload sources)
  local cmds17=(download targets)
  local cmds18=(download sources)
  local cmds19=(download targets)
  local cmds20=(download sources)
  local cmds21=(list project)
  local cmds22=(list sources)
  local cmds23=(list translations)
  local cmds24=(list branches)
  local cmds25=(list languages)
  local cmds26=(status translation)
  local cmds27=(status proofreading)
  local cmds28=(string list)
  local cmds29=(string add)
  local cmds30=(string delete)
  local cmds31=(string edit)
  local cmds32=(glossary list)
  local cmds33=(glossary upload)
  local cmds34=(glossary download)
  local cmds35=(tm list)
  local cmds36=(tm upload)
  local cmds37=(tm download)

  if CompWordsContainsArray "${cmds37[@]}"; then _picocli_crowdin_tm_download; return $?; fi
  if CompWordsContainsArray "${cmds36[@]}"; then _picocli_crowdin_tm_upload; return $?; fi
  if CompWordsContainsArray "${cmds35[@]}"; then _picocli_crowdin_tm_list; return $?; fi
  if CompWordsContainsArray "${cmds34[@]}"; then _picocli_crowdin_glossary_download; return $?; fi
  if CompWordsContainsArray "${cmds33[@]}"; then _picocli_crowdin_glossary_upload; return $?; fi
  if CompWordsContainsArray "${cmds32[@]}"; then _picocli_crowdin_glossary_list; return $?; fi
  if CompWordsContainsArray "${cmds31[@]}"; then _picocli_crowdin_string_edit; return $?; fi
  if CompWordsContainsArray "${cmds30[@]}"; then _picocli_crowdin_string_delete; return $?; fi
  if CompWordsContainsArray "${cmds29[@]}"; then _picocli_crowdin_string_add; return $?; fi
  if CompWordsContainsArray "${cmds28[@]}"; then _picocli_crowdin_string_list; return $?; fi
  if CompWordsContainsArray "${cmds27[@]}"; then _picocli_crowdin_status_proofreading; return $?; fi
  if CompWordsContainsArray "${cmds26[@]}"; then _picocli_crowdin_status_translation; return $?; fi
  if CompWordsContainsArray "${cmds25[@]}"; then _picocli_crowdin_list_languages; return $?; fi
  if CompWordsContainsArray "${cmds24[@]}"; then _picocli_crowdin_list_branches; return $?; fi
  if CompWordsContainsArray "${cmds23[@]}"; then _picocli_crowdin_list_translations; return $?; fi
  if CompWordsContainsArray "${cmds22[@]}"; then _picocli_crowdin_list_sources; return $?; fi
  if CompWordsContainsArray "${cmds21[@]}"; then _picocli_crowdin_list_project; return $?; fi
  if CompWordsContainsArray "${cmds20[@]}"; then _picocli_crowdin_download_sources; return $?; fi
  if CompWordsContainsArray "${cmds19[@]}"; then _picocli_crowdin_download_targets; return $?; fi
  if CompWordsContainsArray "${cmds18[@]}"; then _picocli_crowdin_download_sources; return $?; fi
  if CompWordsContainsArray "${cmds17[@]}"; then _picocli_crowdin_download_targets; return $?; fi
  if CompWordsContainsArray "${cmds16[@]}"; then _picocli_crowdin_upload_sources; return $?; fi
  if CompWordsContainsArray "${cmds15[@]}"; then _picocli_crowdin_upload_translations; return $?; fi
  if CompWordsContainsArray "${cmds14[@]}"; then _picocli_crowdin_upload_sources; return $?; fi
  if CompWordsContainsArray "${cmds13[@]}"; then _picocli_crowdin_upload_translations; return $?; fi
  if CompWordsContainsArray "${cmds12[@]}"; then _picocli_crowdin_pretranslate; return $?; fi
  if CompWordsContainsArray "${cmds11[@]}"; then _picocli_crowdin_tm; return $?; fi
  if CompWordsContainsArray "${cmds10[@]}"; then _picocli_crowdin_glossary; return $?; fi
  if CompWordsContainsArray "${cmds9[@]}"; then _picocli_crowdin_string; return $?; fi
  if CompWordsContainsArray "${cmds8[@]}"; then _picocli_crowdin_status; return $?; fi
  if CompWordsContainsArray "${cmds7[@]}"; then _picocli_crowdin_init; return $?; fi
  if CompWordsContainsArray "${cmds6[@]}"; then _picocli_crowdin_generate; return $?; fi
  if CompWordsContainsArray "${cmds5[@]}"; then _picocli_crowdin_lint; return $?; fi
  if CompWordsContainsArray "${cmds4[@]}"; then _picocli_crowdin_list; return $?; fi
  if CompWordsContainsArray "${cmds3[@]}"; then _picocli_crowdin_pull; return $?; fi
  if CompWordsContainsArray "${cmds2[@]}"; then _picocli_crowdin_download; return $?; fi
  if CompWordsContainsArray "${cmds1[@]}"; then _picocli_crowdin_push; return $?; fi
  if CompWordsContainsArray "${cmds0[@]}"; then _picocli_crowdin_upload; return $?; fi

  # No subcommands were specified; generate completions for the top-level command.
  _picocli_crowdin; return $?;
}

# Generates completions for the options and subcommands of the `crowdin` command.
function _picocli_crowdin() {
  # Get completion data
  local curr_word=${COMP_WORDS[COMP_CWORD]}

  local commands="upload push download pull list lint generate init status string glossary tm pre-translate"
  local flag_opts="-V --version -h --help --no-progress -v --verbose --no-colors"
  local arg_opts=""

  if [[ "${curr_word}" == -* ]]; then
    COMPREPLY=( $(compgen -W "${flag_opts} ${arg_opts}" -- "${curr_word}") )
  else
    local positionals=""
    COMPREPLY=( $(compgen -W "${commands} ${positionals}" -- "${curr_word}") )
  fi
}

# Generates completions for the options and subcommands of the `upload` subcommand.
function _picocli_crowdin_upload() {
  # Get completion data
  local curr_word=${COMP_WORDS[COMP_CWORD]}
  local prev_word=${COMP_WORDS[COMP_CWORD-1]}

  local commands="translations sources"
  local flag_opts="-V --version -h --help --no-progress -v --verbose --no-colors --no-auto-update --dryrun --tree --plain"
  local arg_opts="-T --token --base-url --base-path -i --project-id -s --source -t --translation --dest -c --config --identity -b --branch --label --excluded-language"

  compopt +o default

  case ${prev_word} in
    -T|--token)
      return
      ;;
    --base-url)
      return
      ;;
    --base-path)
      return
      ;;
    -i|--project-id)
      return
      ;;
    -s|--source)
      return
      ;;
    -t|--translation)
      return
      ;;
    --dest)
      return
      ;;
    -c|--config)
      compopt -o filenames
      COMPREPLY=( $( compgen -f -- "${curr_word}" ) ) # files
      return $?
      ;;
    --identity)
      compopt -o filenames
      COMPREPLY=( $( compgen -f -- "${curr_word}" ) ) # files
      return $?
      ;;
    -b|--branch)
      return
      ;;
    --label)
      return
      ;;
    --excluded-language)
      return
      ;;
  esac

  if [[ "${curr_word}" == -* ]]; then
    COMPREPLY=( $(compgen -W "${flag_opts} ${arg_opts}" -- "${curr_word}") )
  else
    local positionals=""
    COMPREPLY=( $(compgen -W "${commands} ${positionals}" -- "${curr_word}") )
  fi
}

