      return
      ;;
    --base-path)
      return
      ;;
    -i|--project-id)
      return
      ;;
    -l|--language)
      return
      ;;
    --branch|-b)
      return
      ;;
  esac

  if [[ "${curr_word}" == -* ]]; then
    COMPREPLY=( $(compgen -W "${flag_opts} ${arg_opts}" -- "${curr_word}") )
  else
    local positionals=""
    COMPREPLY=( $(compgen -W "${commands} ${positionals}" -- "${curr_word}") )
  fi
}

