  local curr_word=${COMP_WORDS[COMP_CWORD]}
  local prev_word=${COMP_WORDS[COMP_CWORD-1]}

  local commands="targets sources"
  local flag_opts="-V --version -h --help --no-progress -v --verbose --no-colors --ignore-match --pseudo --dryrun --tree --skip-untranslated-strings --skip-untranslated-files --export-only-approved --all --plain"
