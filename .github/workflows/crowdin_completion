  local prev_word=${COMP_WORDS[COMP_CWORD-1]}

  local commands=""
  local flag_opts="-V --version -h --help --no-progress -v --verbose --no-colors --skip-untranslated-strings --skip-untranslated-files --export-only-approved --plain"
  local arg_opts="-c --config --identity -T --token --base-url --base-path -i --project-id -l --language --branch -b"

  compopt +o default

  case ${prev_word} in
    -c|--config)
